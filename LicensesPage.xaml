<Window x:Class="Forma_System.LicensesPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Forma_System"
        mc:Ignorable="d"
        Title="LicensesPage" Height="450" Width="800" ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="TextBox_CpuId" HorizontalAlignment="Center" Margin="0,162,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="446" Height="48" TextAlignment="Center"/>
        <TextBlock x:Name="TextBlock_CpuId" HorizontalAlignment="Center" Margin="0,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Text="CpuId" TextAlignment="Center" FontFamily="Times New Roman" FontSize="20"/>
        <TextBox x:Name="TextBox_HddId" HorizontalAlignment="Center" Margin="0,288,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="446" Height="48" TextAlignment="Center"/>
        <TextBlock x:Name="TextBlock_HddId" HorizontalAlignment="Center" Margin="0,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Text="HddId" TextAlignment="Center" FontFamily="Times New Roman" FontSize="20"/>

    </Grid>
</Window>
